{
    "name": "culturekings/laravel5-afterpay",
    "description": "Afterpay API integration exposed via Laravel5 services'",
    "keywords": [
        "laravel5",
        "afterpay",
        "payments",
        "shopping"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jared Fraser",
            "email": "j.fraser@culturekings.com.au"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:culturekings/Symfony2-coding-standard.git"
        }
    ],
    "require": {
        "php": ">=5.6",
        "culturekings/afterpay": "~0.1",
        "illuminate/support": "~5.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.6",
        "culturekings/symfony2-coding-standard": "^2.10",
        "sebastian/phpcpd": "^2.0",
        "satooshi/php-coveralls": "^1.0",
        "orchestra/testbench": "3.2.*",
        "phpunit/phpunit": "^5.5",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "CultureKings\\LaravelAfterpay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests"
        }
    },
    "minimum-stability": "stable"
}
